  description "ACL  entry 1"
  ipv4 source-address 150.150.0.2/32
  actions forwarding-action ACCEPT
  actions log-action LOG_NONE
 !
 acl-entry 2
  ipv4 source-address 151.70.0.2/32
  actions forwarding-action ACCEPT
  actions log-action LOG_NONE
 !
 acl-entry 3
  ipv4 source-address 0.0.0.0/0
  actions forwarding-action ACCEPT
  actions log-action LOG_NONE
 !
!
acl acl-set ipv4_acl_combined1 ACL_IPV4
 acl-entry 10
  description "ACL dscp ttl combined"
  ipv4 source-address 50.0.1.1/32
  ipv4 dscp      22
  ipv4 protocol  IP_TCP
  ipv4 hop-limit 32
  actions forwarding-action DROP
  actions log-action LOG_NONE
 !
 acl-entry 20
  ipv4 source-address 50.0.1.1/32
  ipv4 destination-address 70.0.1.1/32
  ipv4 dscp           24
  ipv4 protocol       IP_UDP
  ipv4 packet-length  494..512
  actions forwarding-action DROP
  actions log-action LOG_NONE
 !
 acl-entry 40
  ipv4 source-address 50.0.1.1/32
  ipv4 destination-address 70.0.1.1/32
  actions forwarding-action ACCEPT
  actions log-action LOG_NONE
 !
 acl-entry 50
  ipv4 source-address 100.1.1.0/24
  actions forwarding-action ACCEPT
  actions log-action LOG_NONE
 !
!
