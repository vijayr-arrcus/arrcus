  ipv6 icmpv6-type        ICMPV6_DST_UNREACH
  ipv6 icmpv6-dst-unreach-code 4
  actions forwarding-action ACCEPT
  actions log-action LOG_NONE
 !
 acl-entry 17006
  description "IP UNREACHABLE"
  ipv6 protocol           IP_ICMPV6
  ipv6 icmpv6-type        ICMPV6_DST_UNREACH
  ipv6 icmpv6-dst-unreach-code 5
  actions forwarding-action ACCEPT
  actions log-action LOG_NONE
 !
