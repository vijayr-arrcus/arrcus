network-instance default
 table-connection DIRECTLY_CONNECTED BGP IPV6
 !
 table-connection DIRECTLY_CONNECTED BGP IPV4
 !
 table-connection STATIC BGP IPV6
 !
 table-connection STATIC BGP IPV4
 !
 protocol BGP default
  global as 4200001001
  global router-id 101.101.101.101
  global afi-safi IPV6_LABELED_UNICAST
   use-maximum-paths eibgp maximum-paths 64
  !
  global afi-safi IPV4_UNICAST
   use-maximum-paths eibgp maximum-paths 64
  !
  global afi-safi IPV4_LABELED_UNICAST
   use-maximum-paths eibgp maximum-paths 64
  !
  global afi-safi IPV6_UNICAST
   use-maximum-paths eibgp maximum-paths 64
  !
  neighbor 12.12.12.2
   peer-as 4200001002
   bfd enable false
   afi-safi IPV4_UNICAST
    apply-policy import-policy [ import-policy-2 ]
    apply-policy export-policy [ export-policy-1 ]
    exit
   !
   afi-safi IPV4_LABELED_UNICAST
    apply-policy import-policy [ import-policy-2 ]
    apply-policy export-policy [ export-policy-1 ]
    exit
   !
   exit
  !
  neighbor 13.13.13.2
   peer-as 4200001002
   bfd enable true
   afi-safi IPV4_UNICAST
    apply-policy import-policy [ import-policy-2 ]
    apply-policy export-policy [ export-policy-1 ]
    exit
