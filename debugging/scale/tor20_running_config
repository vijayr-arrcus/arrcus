routing-policy policy-definition import-policy-1
 statement 01
  conditions match-prefix-set prefix-set ipv4-prefix-1
  actions accept-route
  actions bgp-actions set-route-origin EGP
  actions bgp-actions set-local-pref 101
  actions bgp-actions set-med 50
  actions bgp-actions set-as-path-prepend repeat-n 5
  actions bgp-actions set-community method REFERENCE
  actions bgp-actions set-community options ADD
  actions bgp-actions set-community reference community-set-ref C22
  actions bgp-actions set-ext-community method REFERENCE
  actions bgp-actions set-ext-community options ADD
  actions bgp-actions set-ext-community reference ext-community-set-ref E22
  actions bgp-actions set-large-community method REFERENCE
  actions bgp-actions set-large-community options ADD
