qos classifier qos_classifier_1
 filter LOCAL_TC
  local-tc-value [ 1 ]
 !
!
qos classifier qos_classifier_2
 filter LOCAL_TC
  local-tc-value [ 2 ]
 !
!
qos classifier qos_classifier_3
 filter LOCAL_TC
  local-tc-value [ 3 ]
 !
!
qos classifier qos_classifier_4
 filter LOCAL_TC
  local-tc-value [ 4 ]
 !
!
qos policy qos-policy-1
 classifier qos_classifier_1
  action RATE_EXCESS
   ratio 1
  !
 !
 classifier qos_classifier_2
  action RATE_EXCESS
   ratio 2
  !
 !
 classifier qos_classifier_3
  action RATE_MIN
   rate value 1
   rate unit gbps
  !
  action RATE_MAX
   rate value 1
   rate unit gbps
  !
 !
 classifier qos_classifier_4
  action PRIORITY
   level 1
  !
 !
!
qos tablemap dscp-to-tclass-1
 to-type   LOCAL_TC
 from-type DSCP
 local-tc-entry 1 0
  from-value [ 20 ]
 !
 local-tc-entry 2 0
  from-value [ 22 ]
 !
