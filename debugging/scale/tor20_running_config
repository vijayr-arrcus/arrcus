event ON_ARCOS_START
 description ON_ARCOS_START
 event-entry 0
  execute "rm -fr /tmp/ON_CONFIG_START_1; date  > /tmp/ON_ARCOS_START_0"
 !
 event-entry 100
  execute "rm -fr /tmp/ON_CONFIG_START_1; date  > /tmp/ON_ARCOS_START_100"
 !
